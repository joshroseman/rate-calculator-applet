{
  "version": 2,
  "name": "rate-calculator-applet",
  "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/quotes/([^/]+)",
      "dest": "/api/v1/quotes/[id].ts"
    },
    {
      "src": "/api/v1/quotes",
      "dest": "/api/v1/quotes.ts"
    }
  ]
}
